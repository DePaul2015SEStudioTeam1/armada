<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.depaul.armada.domain.Preference" table="preferences" >
	
		<id column="id" name="id" type="java.lang.Long">
		  <generator class="native"/>
		</id>
		<property column="pref_key" name="key" type="java.lang.String" not-null="true"/>
		<property column="pref_value" name="value" type="java.lang.String" unique="true" not-null="true"/>
	</class>
</hibernate-mapping>