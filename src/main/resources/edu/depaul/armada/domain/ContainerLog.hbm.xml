<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.depaul.armada.domain.ContainerLog" table="container_log">
		<id column="id" name="id" type="java.lang.Long"/>
		<property column="container_id" name="containerId" type="java.lang.String" />
		<property column="timestamp" name="timestamp" type="java.lang.String" />
		<property column="mem_usage" name="memUsage" type="java.lang.Long" />
		<property column="total_cpu_usage" name="totalCpuUsage" type="java.lang.Long" />
		<property column="filesystem_usage" name="filesystemUsage" type="java.lang.Long" />
	</class>
</hibernate-mapping>