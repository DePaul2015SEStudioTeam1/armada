<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.depaul.armada.domain.Container" table="container" >
	
		<id column="id" name="id" type="java.lang.Long">
		  <generator class="native"/>
		</id>
		<property column="name" name="name" type="java.lang.String" />
		<property column="container_unique_id" name="containerUniqueId" type="java.lang.String" />
		<property column="cadvisor_url" name="CAdvisorURL" type="java.lang.String" />
		<bag name="logs" cascade="all">
		  <key column="container_id"/>
		  <one-to-many class="edu.depaul.armada.domain.ContainerLog"/>
		</bag>
	</class>
</hibernate-mapping>