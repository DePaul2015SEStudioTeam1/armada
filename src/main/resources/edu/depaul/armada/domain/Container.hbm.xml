<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.depaul.armada.domain.Container" table="container" >
	
		<id column="id" name="id" type="java.lang.Long">
		  <generator class="native"/>
		</id>
		<property column="name" name="name" type="java.lang.String" />
		<property column="container_id" name="containerId" type="java.lang.String" />
		<property column="cadvisor_url" name="cAdvisorURL" type="java.lang.String" />
		<property column="mem_limit" name="memLimit" type="java.lang.Long" />
		<property column="cpu_limit" name="cpuLimit" type="java.lang.Long" />
		<property column="filesystem_capacity" name="filesystemCapacity" type="java.lang.Long" />
	</class>
</hibernate-mapping>